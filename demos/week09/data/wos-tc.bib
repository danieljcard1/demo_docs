@article{ WOS:000370733100001,
Author = {Broberg, Mats S. E.},
Title = {A Decade of XML-and a New Procurement and Lessons Learned},
Journal = {TECHNICAL COMMUNICATION},
Year = {2016},
Volume = {63},
Number = {1},
Pages = {6-22},
Month = {FEB},
Abstract = {Purpose: To describe the specification, procurement, and implementation
   of a new, XML-based technical documentation system for a large,
   multinational company.
   Method: This study uses a descriptive analytical method, based on
   available project data, interviews, and fault analyses.
   Results: The results of this research outline the technical, strategic,
   and organizational aspects that are associated with the procurement of a
   critical multi-site system where a high level of availability is
   necessary.
   Conclusion: Organizational stakeholders were identified as critical for
   success; new authors' previous experiences lacked predictive value;
   existing documentation cultures at one specific site were not studied
   closely enough; and a deeper analysis of risk management with regard to
   the choice of DTD should have been made.},
Type = {Article},
Keywords = {XML; content management system; procurement; implementation; migration},
