@article{ WOS:000388291600005,
Author = {St Amant, Kirk and Meloncon, Lisa},
Title = {Reflections on Research: Examining Practitioner Perspectives on the
   State of Research in Technical Communication},
Journal = {TECHNICAL COMMUNICATION},
Year = {2016},
Volume = {63},
Number = {4},
Pages = {346-364},
Month = {NOV},
Abstract = {Purpose: While research is an important to both academics and
   practitioners alike, it seems the field is currently at an impasse about
   what constitutes research and what questions should be a priority for
   the field. We wanted to give practitioners a forum to provide their
   perspectives on what research is and what questions the field should be
   researching.
   Method: We conducted 30 asynchronous interviews with practitioners, and
   then analyzed the interviews for common themes and topics.
