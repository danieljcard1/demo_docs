@article{ WOS:000419384400002,
Author = {Shivers-McNair, Ann and Diego, Clarissa San},
Title = {Localizing Communities, Goals, Communication, and Inclusion: A
   Collaborative Approach},
Journal = {TECHNICAL COMMUNICATION},
Year = {2017},
Volume = {64},
Number = {2},
Pages = {97-112},
Month = {MAY},
Abstract = {Purpose: The authors, one a researcher and one an international
   community strategy practitioner, illustrate community strategy work as a
   multi-faceted localization practice that intersects with user experience
   design and user localization. We argue that localized community strategy
   is crucial not only to practice that aims for inclusivity and social
   justice but also to research and theory building that aims for
   inclusivity and social justice.
   Method: We model a dialogic, localized knowledge-making process that
   operates at the level of collaborative research between a theorist and a
   practitioner and at the level of international practice. The authors
   collaboratively analyzed the experiences of the practitioner, as well as
   our own collaboration process, and coded them for key dimensions, which
   can guide and be further developed both in practice and in research.
   Results: The authors identify and discuss four key dimensions of a
   community strategist's localization practices-localizing communities,
   localizing goals, localizing communication, and localizing inclusion-and
   illustrate them both in our own collaborative practice of analysis and
   in the practice of international community strategy.
   Conclusion: The definitions of user, community, and diversity themselves
   must be continually localized in our work to engage across cultures and
   across theory and practice. We call for further collaboration and
   research at the intersections of international community strategy work
   with technical communication and global user experience, particularly
   the role of localizing diversity and inclusion.},
Type = {Article},
Keywords = {community strategy; user localization; diversity; inclusion; social
   justice},
Keywords-Plus = {TECHNICAL COMMUNICATION; LOCALIZATION},
