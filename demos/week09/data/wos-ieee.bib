@article{ WOS:000235985300002,
Author = {Mackiewicz, J},
Title = {The functions of formulaic and nonformulaic compliments in interactions
   about technical writing},
Journal = {IEEE TRANSACTIONS ON PROFESSIONAL COMMUNICATION},
Year = {2006},
Volume = {49},
Number = {1},
Pages = {12-27},
Month = {MAR},
Abstract = {Writing tutors are encouraged to use compliments in their interactions
   with technical writing students. However the form of compliments
   strongly influences how they function. Specifically, formulaic
   compliments like ``It's good{''} function differently from nonformulaic
   compliments like `The size is excellent in terms of visually aiding the
   reader.{''} A total of 107 compliments were analyzed from 13
   interactions between 12 writing tutors and 12 engineering students.
   About 61\% of tutors' compliments followed one of six formulae, and
   about 39\% were nonformulaic. Formulaic compliments were general and
   mainly performed a phatic function, filling pauses and avoiding silence,
   particularly in interaction closings. Nonformulaic compliments were more
   specific and individualized, and they may, therefore, be more
   instructive thanformulaic compliments. Nonformulaic compliments also
   performed an exploratory function, allowing participants to renegotiate
   discourse status. This study points to other avenues of research,
   particularly research that systemically examines writers' perceptions of
   formulaic and nonformulaic feedback, such as compliments.},
Type = {Article},
DOI = {10.1109/TPC.2006.870461},
Keywords = {compliments; engineering students; formulaic language; technical
   writing; tutors},
