@article{ WOS:000278038600004,
Author = {Anandarajan, Murugan and Zaman, Maliha and Dai, Qizhi and Arinze, Bay},
Title = {Generation Y Adoption of Instant Messaging: An Examination of the Impact
   of Social Usefulness and Media Richness on Use Richness},
Journal = {IEEE TRANSACTIONS ON PROFESSIONAL COMMUNICATION},
Year = {2010},
Volume = {53},
Number = {2},
Pages = {132-143},
Month = {JUN},
Abstract = {By integrating Media Richness Theory, Channel Expansion Theory, and the
   Technology Acceptance Model (TAM), we study the postadoption use
   behavior of instant messaging. We developed the construct ``use
   richness{''} as a measure of the extent to which users use the media
   communication capacity after adoption and proposed a conceptual model of
   the antecedents of use richness. Through a field survey with 272 valid
   responses and structural equation modeling, we empirically tested our
   model and found that use richness is positively affected by perceived
   media richness, perceived usefulness, and perceived social usefulness.},
