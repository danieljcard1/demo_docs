@article{ WOS:000165754600004,
Author = {Adler, TR},
Title = {An evaluation of the social perspective in the development of technical
   requirements},
Journal = {IEEE TRANSACTIONS ON PROFESSIONAL COMMUNICATION},
Year = {2000},
Volume = {43},
Number = {4},
Pages = {386-396},
Month = {DEC},
Abstract = {This study used a qualitative methodology to examine how discourse norms
   and socialization processes affect the development of technical
   requirements. Our exploratory investigation of how government personnel
   develop and review technical requirements indicates that discourse norms
   and academic technical writing socialization processes affect the
   technical writing process. Technical writers perceived that requirements
   in work statements became less precise as more requirements were
   coordinated in team-based designs. In essence, we found that in
   team-based designs, INTERPRETATION CONFLICT and TECHNICAL DIFFUSION were
   important dimensions when writing and coordinating technical
   requirements. Our findings suggest that collaborative technical writing
   is a complex and difficult process in team-based designs where
   integration and persuasion skills dominate.},
Type = {Article},
DOI = {10.1109/47.888813},
Keywords = {discourse norms; interpretation conflict; socialization processes;
   team-based designs; technical diffusion; technical requirements},
