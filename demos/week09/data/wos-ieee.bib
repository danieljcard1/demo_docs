@article{ WOS:000320128300006,
Author = {Grant, Lynn},
Title = {The Frequency and Function of <i>Just</i> in British and New Zealand
   Engineering Lectures},
Journal = {IEEE TRANSACTIONS ON PROFESSIONAL COMMUNICATION},
Year = {2013},
Volume = {56},
