@article{ WOS:000179690100004,
Author = {Van Luxemburg, APD and Ulijn, JM and Amare, N},
Title = {The contribution of electronic communication media to the design
   process: Communicative and cultural implications},
Journal = {IEEE TRANSACTIONS ON PROFESSIONAL COMMUNICATION},
Year = {2002},
Volume = {45},
Number = {4},
Pages = {250-264},
Month = {DEC},
Abstract = {Innovation in a company's design process is increasingly a matter of
   cooperation between the company and its customers. New Information and
   Communication Technology (ICT) possibilities such as electronic
   communication (EC) media generate even more opportunities for companies
   to collaborate with customers during the early stages of research and
   development. This exploratory study examined the design process of five
   Dutch firms and the cultural and communicative implications of
   cooperation in the design process between the supplier and the customer
   using EC media. We found that the selected use of EC media for
   communication between R\&D and customers has a positive effect on the
   design process. We also discovered that the characteristics of the most
   suitable EC media depend on the design activity, and that the corporate
   and professional cultures of both the company and its customers involved
   in the cooperation seem to affect the communication media used. Finally,
   the future use of new ICT in the design area is discussed.},
Type = {Article},
DOI = {10.1109/TPC.2002.805152},
Keywords = {design process; electronic communication media; information and
   communication technology; innovation management; national culture;
   professional culture},
