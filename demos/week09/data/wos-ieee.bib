@article{ WOS:000433350300007,
Author = {Weissbach, Robert S. and Pflueger, Ruth C.},
Title = {Collaborating With Writing Centers on Interdisciplinary Peer Tutor
   Training to Improve Writing Support for Engineering Students},
Journal = {IEEE TRANSACTIONS ON PROFESSIONAL COMMUNICATION},
