@article{ WOS:000335380400003,
Author = {Kim, Eun Gyong and Shin, Abigail},
Title = {Seeking an Effective Program to Improve Communication Skills of
   Non-English-Speaking Graduate Engineering Students: The Case of a Korean
   Engineering School},
Journal = {IEEE TRANSACTIONS ON PROFESSIONAL COMMUNICATION},
Year = {2014},
Volume = {57},
Number = {1},
Pages = {41-55},
Month = {MAR},
Abstract = {Research problem: Many Asian universities have begun reforms to enhance
   educational competitiveness in our globalizing economy. This study aims
   to ascertain the status of English communication education and
   English-medium instruction at a Korean engineering school and to offer
   workable suggestions for English communication training for Korean
   graduate engineering students. Research questions: Should English
   communication education be offered at the graduate level in Korean
   engineering schools? How could English communication education be
   improved for Korean graduate engineering students? Literature review:
