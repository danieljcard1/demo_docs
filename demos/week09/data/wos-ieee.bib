@article{ WOS:000089222100006,
Author = {Evans, MB},
Title = {Challenges in developing research-based web design guidelines},
Journal = {IEEE TRANSACTIONS ON PROFESSIONAL COMMUNICATION},
Year = {2000},
Volume = {43},
Number = {3},
Pages = {302-312},
Month = {SEP},
Abstract = {Formulating design guidelines from research results can potentially
   offer two advantages: available empirical knowledge is distilled into a
   readily accessible form, and practitioners can judge the authority of
   those guidelines for themselves. But a case history demonstrates that
   challenges confront the der eloper of research-based guidelines for
   website design. First, few studies of people using the Web have yet been
   conducted. Second, when the process of guideline development is not
   systematic and designed to control bias and subjectivity, the resulting
   guidelines can be compromised. This paper describes the issues and
   problems one guideline developer encountered and suggests a guideline
   development process that might resolve them.},
Type = {Article},
