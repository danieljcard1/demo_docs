@article{ WOS:000187736200002,
Author = {Nelson, S},
Title = {Engineering and technology student perceptions of collaborative writing
   practices},
Journal = {IEEE TRANSACTIONS ON PROFESSIONAL COMMUNICATION},
Year = {2003},
Volume = {46},
Number = {4},
Pages = {265-276},
Month = {DEC},
Abstract = {Results are presented from an assessment of student perceptions of
   collaborative writing practices before and after taking an upper
   division professional writing class. While most of the classes
   introduced students to these writing practices, several did not. The
   assessment was both quantitative and qualitative. Whether or not they
   had prior experience in the classroom, all students generally reported
   that they are likely to seek out opportunities to use both. peer review
   and collaborative writing processes once they enter the workplace.
   However, students who are exposed to these practices in a classroom
   setting are more likely to report that they intend to continue these
   practices in the workplace.},
