@article{ WOS:000258916100002,
Author = {Leydens, Jon A.},
Title = {Novice and insider perspectives on academic and workplace writing:
   Toward a continuum of rhetorical awareness},
Journal = {IEEE TRANSACTIONS ON PROFESSIONAL COMMUNICATION},
Year = {2008},
Volume = {51},
Number = {3},
Pages = {242-263},
Month = {SEP},
Abstract = {Communication researchers have helped frame understandings about
   disciplinary and professional writing. But often they are outsiders
   looking in. To complement that research, this study focuses on insider
   perspectives of engineers in academic and industrial contexts at diverse
   career stages. Qualitative data are analyzed using phenomenological
   research methods. Findings indicate that participant perspectives fall
   along a rhetorical awareness continuum at points spanning from denial
   and acknowledgment to an accentuation of rhetoric as critical to
   individual and organizational Success. Participant perspectives along
   the continuum also vary in terms of writer and reader roles. writer
   identity, career stage/organizational role. and objectivity.
   Implications for practitioners are discussed.},
Type = {Article},
DOI = {10.1109/TPC.2008.2001249},
