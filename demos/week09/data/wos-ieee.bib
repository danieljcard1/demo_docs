@article{ WOS:000263920100004,
Author = {Lee, Zoonky and Lee, Younghwa},
Title = {Emailing the Boss: Cultural Implications of Media Choice},
Journal = {IEEE TRANSACTIONS ON PROFESSIONAL COMMUNICATION},
Year = {2009},
Volume = {52},
Number = {1},
Pages = {61-74},
Month = {MAR},
Abstract = {This paper applies Media Richness Theory and Social Influence Theory in
   different countries where the significance of media is uniquely shaped
   by tire culture. In particular we focus on whether Media Richness Theory
   and Social Influence Theory hold for communication between subordinates
   and supervisors in different cultures. To test this hypothesis, a
   comparative cross cultural field study with knowledge workers (n = 120)
   in the telecommunication industry in the United States and South Korea
   was conducted. This study demonstrates that country, task equivocality,
   and communication direction are the factors that affect individuals'
   media choice. Communication direction was found to be the strongest
   factor influencing media choice for Korean employees, whereas task
   equivocality was the dominant factor influencing media choice for LIS
   employees. This study also demonstrates the influence of national
   culture on media choice among US and Korean employees. Implications for
   both theory and practice are discussed.},
