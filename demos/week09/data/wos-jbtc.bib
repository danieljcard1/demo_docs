@article{ WOS:000263912800004,
Author = {Spilka, Rachel},
Title = {Practitioner Research Instruction A Neglected Curricular Area in
   Technical Communication Undergraduate Programs},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
Year = {2009},
Volume = {23},
Number = {2},
Pages = {216-237},
Month = {APR},
Abstract = {Most technical communication practitioners conduct research throughout
   their careers. Yet, a survey of the Web sites of 114 undergraduate
   technical communication programs between September 2006 and April 2007
   revealed that 65\% (about two thirds) of these programs are providing
   minimal or no exposure to research instruction and therefore are not
   sufficiently preparing students to handle the types of research they
   will encounter in their upcoming careers. Given the disconnect between
   the centrality of research in the work that technical communicators do
   and the low presence of research instruction at the undergraduate level,
   academics need to look for ways to overcome institutional and other
   constraints in order to give research training greater priority in their
   undergraduate programs.},
Type = {Article; Proceedings Paper},
DOI = {10.1177/1050651908328882},
