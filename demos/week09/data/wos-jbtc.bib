@article{ WOS:000168177000002,
Author = {Artemeva, N and Freedman, A},
Title = {``Just the boys playing on computers{''} - An activity theory analysis
   of differences in the cultures of two engineering firms},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
Year = {2001},
Volume = {15},
Number = {2},
Pages = {164-194},
Month = {APR},
Abstract = {Using activity theory as a supplement to genre studies, this article
   explores a case of the disintegration of a traditional engineering firm.
   It focuses on the causes of such disintegration and the role of
   different types of communication in serving as sites where
   contradictions can be brought to visibility and resolution. The authors'
   goal is both to show the power of activity theory in illuminating issues
   of tension, contradiction, and dissonance that lead to the breakup of
   the original organization into true separate firms and point to
   fundamental differences in the cultures of traditional engineering firms
   and software design enterprises.},
