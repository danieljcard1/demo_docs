@article{ WOS:A1996UX79900004,
Author = {Larson, MH},
Title = {Patterns in transition - A writing teacher's survey of organizational
   socialization},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
Year = {1996},
Volume = {10},
Number = {3},
Pages = {352-368},
Month = {JUL},
Abstract = {Research in organizational socialization outlines a common process of
   transition making. Newcomers first anticipate what the workplace and
   their involvement there will be like and then adjust these expectations
   upon encounter with organizational reality Encounter often brings some
   disappointment, so struggles with motivation must be resolved before the
   initiates are ready to settle in and become contributing members. A
   survey of this research, illustrated with case study excerpts from
   undergraduate student interns, suggests that classes intended to prepare
   students for workplace communication can do so more effectively if they
   make students aware of this adjustment process and if they help students
   explore the possible writing implications of such nonwriting issues.},
