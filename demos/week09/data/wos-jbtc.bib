@article{ WOS:A1995RH28700002,
Author = {BLYLER, NR},
Title = {PEDAGOGY AND SOCIAL-ACTION - A ROLE FOR NARRATIVE IN PROFESSIONAL
   COMMUNICATION},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
Year = {1995},
Volume = {9},
Number = {3},
Pages = {289-320},
Month = {JUL},
Abstract = {Scholars in professional communication have called for a reexamination
   of pedagogy, asking that it instruct students not simply in the forms of
   work place discourse but also in the connections between that discourse
   and socially responsible communicative action. This article posits that
   narrative can provide a basis for a pedagogy of social action-for a
   pedagogy, that is, that enables students to understand the workings of
   power and cultural reproduction in professional settings and that
   fosters reflection, critique, and dialogue. The article first reviews
   narrative theory supporting this claim, then discusses ways that
   teachers can use narrative to help students critique examples of
   professional discourse and their own composing choices. The article
   closes by discussing both the concerns about and the possibilities for
   such a pedagogy.},
