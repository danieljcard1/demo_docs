@article{ WOS:000469379100004,
Author = {Sharp, Matthew R.},
Title = {Agency, Authority, and Epideictic Rhetoric: A Case Study of Bottom-Up
   Organizational Change},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
Year = {2019},
Volume = {33},
Number = {3},
Pages = {338-369},
Month = {JUL},
Abstract = {By analyzing a case study of organizational decision making at a large
   research university, this article argues that the agency to make a
   difference within organizations-to effect organizational change-is not
   exclusive to those in positions of authority. This case study
   demonstrates how subordinate members of a university affected
   management's decision-making process through their use of rhetorical
   identification. Specifically, these organizational members gained this
   agency by reproducing certain values and identities through epideictic
   rhetoric in order to encourage collective action and effect
   organizational change from the bottom up.},
