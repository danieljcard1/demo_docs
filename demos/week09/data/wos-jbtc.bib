@article{ WOS:000507171800001,
Author = {Rogers, Priscilla S. and Pawlik, Lisa A. and Shwom, Barbara L.},
Title = {Formal Communications' Role in Knowledge Work: Evidence From Projects},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
Year = {2020},
Volume = {34},
Number = {2},
Pages = {115-156},
Month = {APR},
Abstract = {To investigate the contribution of formal communications (FCs) to
   problem-solving knowledge work, this study examines survey, interview,
   and observational data from 212 teams who produced contracting
   documents, reports, and PowerPoint presentations while working on
   projects for diverse organizations worldwide. The study found that these
   FCs engaged teams in a contextual-conceptual dynamic involving
   interactive pairs of integral work activities. The findings validate,
   integrate, and extend prior scholarship on organizational genres,
   writing to learn, and the role of material texts in the work process,
   leading to a comprehensive framework that pinpoints opportunities for
   managing FCs to achieve their fullest potential.},
