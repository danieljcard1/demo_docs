@article{ WOS:000342578800002,
Author = {Marsen, Sky},
Title = {``Lock the Doors{''}: Toward a Narrative-Semiotic Approach to
   Organizational Crisis},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
