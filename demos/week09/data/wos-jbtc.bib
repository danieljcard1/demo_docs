@article{ WOS:000355335800002,
Author = {Welhausen, Candice A.},
Title = {Power and Authority in Disease Maps: Visualizing Medical Cartography
   Through Yellow Fever Mapping},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
