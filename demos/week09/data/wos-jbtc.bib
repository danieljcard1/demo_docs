@article{ WOS:000524176600001,
Author = {Baker, Matthew J.},
Title = {Contextual Information in Social How-To Questions That Initiate
   Documentation},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
Year = {2020},
Volume = {34},
Number = {3},
Pages = {287-326},
Month = {JUL},
Abstract = {This study introduces social question-and-answer (SQA) documentation to
   technical and professional communication scholarship. It conceptualizes
   SQA as interactive, user-generated documentation and describes
   contextual information types within social how-to questions that
   initiate documentation. It also explores whether contextual information
   associates with answers that complete the interactive documentation.
   Results reliably describe 15 information types based on content analysis
   of 3,529 contextual information types from 500 questions. Exploratory
   statistical analysis suggests that askers may increase answerability by
   including less speculative thought, more error messages, and less
   general situation information. To facilitate complete SQA documentation,
   the study calls for additional research into question content and
   answerability.},
Type = {Article},
DOI = {10.1177/1050651920910226},
