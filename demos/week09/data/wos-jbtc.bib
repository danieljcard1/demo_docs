@article{ WOS:000295210100004,
Author = {Spinuzzi, Clay},
Title = {Losing by Expanding: Corralling the Runaway Object},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
Year = {2011},
Volume = {25},
Number = {4},
Pages = {449-486},
Month = {OCT},
Abstract = {Third-generation activity theory (3GAT) has become a popular theoretical
   and methodological framework for writing studies, particularly in
   technical communication. 3GAT involves identifying an object, a material
   or problem that is cyclically transformed by collective activity. The
   object is the linchpin of analysis in the empirical case. Yet the notion
   of object has expanded methodologically and theoretically over time,
   making it difficult to reliably bound an empirical case. In response,
   this article outlines the expansion of the object, diagnoses this
   expansion, and proposes an alternate approach that constrains the object
   for case-study research in writing studies.},
Type = {Article},
DOI = {10.1177/1050651911411040},
