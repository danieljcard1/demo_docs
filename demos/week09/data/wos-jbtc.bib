@article{ WOS:000187530100002,
Author = {Jeyaraj, J},
Title = {Liminality and othering - The issue of rhetorical authority in technical
   discourse},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
Year = {2004},
Volume = {18},
Number = {1},
Pages = {9-38},
Month = {JAN},
Abstract = {Subject matter experts, under the influence of modernist notions of
   authorship, often view technical writers as mere grammar and punctuation
   specialists and marginalize them as their ignorant ``other.{''}
   Technical writers, on the other hand, as rhetoricians occupying a
   liminal space between different disciplines, can understand different
   disciplinary rhetorics. If subject matter experts, instead of
   marginalizing technical writers, would view them as liminal subjects who
   are knowledgeable in different disciplinary rhetorics, then technical
   writers, through liminal practice, may be able to use their knowledge of
   audience and rhetoric to improve the quality of documentation.},
Type = {Article},
