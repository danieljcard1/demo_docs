@article{ WOS:000220363700001,
Author = {Anderson, DL},
Title = {The textualizing functions of writing for organizational change},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
Year = {2004},
Volume = {18},
Number = {2},
Pages = {141-164},
Month = {APR},
Abstract = {This article examines the role of writing during an attempt at
   organizational change. Through the investigation of conversational and
   writing practices used by members of a project team at a high-tech
   corporation, the article argues that writing has a textualizing
   function. In the context of members' work toward organizational change,
   writing served as a textualizing practice that documented, fixed, and
   stabilized ideas developed in conversation. Written forms that create
   general truths out of individual experiences help both to define the
   organizational change to come and to create the change as an object to
   be distributed and consumed by organizational members. The results of
   the study describe how writing helps to stabilize organizational reality
   to enable change to occur.},
Type = {Article},
DOI = {10.1177/1050651903260800},
Keywords = {organizational change; writing; discourse; textualization},
