@article{ WOS:000231854400001,
Author = {Artemeva, N},
Title = {A time to speak, a time to act a rhetorical genre analysis of a novice
   engineer's calculated risk taking},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
Year = {2005},
Volume = {19},
Number = {4},
Pages = {389-421},
Month = {OCT},
Abstract = {This article discusses a longitudinal case study of a novice engineer
   who has successfully challenged a workplace genre. The study shows that
   a combination of the novice's family background, a university
   engineering communication course, and workplace experiences helped him
   achieve success. It also provides evidence that, even though genres may
   differ from workplace to workplace, experienced professionals do
   recognize and accept superior communication practices imported from
   elsewhere. Thus, best practices may be taught apart from local contexts.
   The case study allows technical communication instructors and
   researchers to refine current understanding of what mastering genres
   means and indicates directions for the development of new pedagogics.},
