@article{ WOS:000166406300003,
Author = {Guiniven, JE},
Title = {The lessons of survivor literature in communicating decisions to
   downsize},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
Year = {2001},
Volume = {15},
Number = {1},
Pages = {53-71},
Month = {JAN},
Abstract = {Many companies have entered a new era of human resources management-one
   based on transaction cost economics and one in which downsizing has
   become a permanent part of the corporate landscape. But their insistence
   on communicating decisions to downsize solely in economic terms is
   creating serious problems among employees who survive the layoffs.
   Disloyalty disaffection, increased absenteeism, and even acts of
   sabotage are growing among workers who view downsizing as a social, not
   economic, issue. This article discusses the new era of human resources
   management and reviews survivor literature in an effort to provide
   guidance to companies about how to communicate downsizing, specifically
   and how to communicate with the postdownsized workforce, generally.},
