@article{ WOS:000432987100002,
Author = {Batova, Tatiana},
Title = {Work Motivation in the Rhetoric of Component Content Management},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
Year = {2018},
Volume = {32},
Number = {3},
Pages = {308-346},
Month = {JUL},
Abstract = {This article describes a 12-month qualitative study that analyzes how a
   company's transition to component content management (CCM)a set of
   methodologies, processes, and technologies that allows working with
   texts as small components rather than complete, static
   documentsinfluences the workmotivation of its technical communicators.
   The analysis is based on actor-network theory and the theories of work
   motivation from economics. When technical communicators felt that CCM's
   only focus was efficiency and savings and that they were not recognized
   and connected to the fruits of their labor, their motivation decreased.
   But their motivation increased when they were engaged in job
   craftingreshaping their understanding of the fruits of their labor and
   gaining recognition through prosocial behavior.},
Type = {Article},
DOI = {10.1177/1050651918762030},
Keywords = {component content management; work motivation; job crafting},
