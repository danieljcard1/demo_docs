@article{ WOS:000231854400004,
Author = {Starke-Meyerring, D},
Title = {Meeting the challenges of globalization a framework for global
   literacies in professional communication programs},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
