@article{ WOS:000629720600001,
Author = {Zhang, Huiyu and Song, Yiqun and Wei, Yuanhong and Liu, Jingjiang},
Title = {Discursive Communication Strategies for Introducing Innovative Products:
   The Content, Cohesion, and Coherence of Product Launch Presentations},
