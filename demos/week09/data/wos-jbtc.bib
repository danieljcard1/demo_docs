@article{ WOS:000507089900001,
Author = {Thoms, Claudia and Degenhart, Anke and Wohlgemuth, Katharina},
Title = {Is Bad News Difficult to Read? A Readability Analysis of Differently
   Connoted Passages in the Annual Reports of the 30 DAX Companies},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
Year = {2020},
Volume = {34},
Number = {2},
Pages = {157-187},
Month = {APR},
Abstract = {This study examines the strategic use of readability to obfuscate
   negative news in a German financial communication context. Combining a
   manual and an automated content analysis, the authors assess the tone
   and readability of three parts (chairman's address, share-price
   development, and development in the fiscal year) of the 2014 annual
   reports of the 30 companies listed in the German stock index DAX. The
   results indicate that positively connoted passages in annual reports are
   not necessarily easier to read than negatively connoted passages.
   Furthermore, the readability of the annual report varies depending on
   the part and its function within the report.},
Type = {Article},
