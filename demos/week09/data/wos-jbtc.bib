@article{ WOS:000523918200001,
Author = {Gubala, Carolyn and Larson, Kara and Meloncon, Lisa},
Title = {Do Writing Errors Bother Professionals? An Analysis of the Most
   Bothersome Errors and How the Writer's Ethos is Affected},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
Year = {2020},
Volume = {34},
Number = {3},
Pages = {250-286},
Month = {JUL},
Abstract = {This study asks whether grammatical and mechanical errors bother
   business professionals, which of these types of errors are most
   bothersome, and whether such errors affect perceptions of the writer and
   their ethos. We administered a 17-question survey to roughly 100
   business professionals whose roles are not primarily writing and
   communication within organizations. The findings show that business
   professionals are bothered by these errors and that the level of
   bothersomeness has increased from previous studies. Additionally, the
   findings show that participants have clear views of writers who make
   errors and that the context of the error matters. The authors conclude
   by offering implications for technical and professional communication.},
