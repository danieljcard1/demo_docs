@article{ WOS:000570647200001,
Author = {Lambrecht, Kathryn},
Title = {Tracking the Differentiation of Risk: The Impact of Subject Framing in
   CDC Communication Regarding COVID-19},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
Year = {2021},
Volume = {35},
Number = {1, SI},
Pages = {94-100},
Month = {JAN},
Abstract = {Communicating risk amid moments of scientific ambiguity requires
   balance: Overdelivering certainty levels can cause undue alarm whereas
   underdelivering them can lead to increased public risk. Despite this
   complexity, risk assessment is an important decision-making tool. This
   article analyzes the circulation of the term ``risk{''} in a corpus
   (74,804 words) of Centers for Disease Control and Prevention
   communications regarding COVID-19 from January 1 to April 30, 2020.
   Tracking collocations of the 147 instances of risk in this corpus
   reveals that experts initially framed risk away from individuals,
   complicating people's differentiation between public and personal
   impacts. Recommendations are offered for how institutions can reframe
   subjectivity to promote vigilance during pandemics.},
Type = {Article},
DOI = {10.1177/1050651920958394},
