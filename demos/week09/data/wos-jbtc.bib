@article{ WOS:001009453000001,
Author = {Carlson, Erin Brock and Overmyer, Trinity},
Title = {Tuning to Place: Using Photos to Better Understand Problems in Technical
   Communication Classes},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
Year = {2023},
Volume = {37},
Number = {4},
Pages = {378-408},
Month = {OCT},
Abstract = {This article highlights the role of place in understanding problems,
   specifically within community-engaged projects in upper-level technical
   and professional communication courses. Drawing on a year-long
   participant-generated imagery study with students, instructors, and
   community partners, the authors argue that photographic research is
   effective in helping participants and researchers tune to place. Taking
   photos offers opportunities for documentation, individual
   interpretation, and collaborative reflection, resulting in a deeper,
   more nuanced sense of place. Ultimately, this article demonstrates how a
   greater awareness of place, cultivated through reflecting on visual
   evidence, enhances engagement projects and helps technical communicators
   address complex problems.},
