@article{ WOS:000355335600002,
Author = {Shin, Wonsun and Pang, Augustine and Kim, Hyo Jung},
Title = {Building Relationships Through Integrated Online Media: Global
   Organizations' Use of Brand Web Sites, Facebook, and Twitter},
Journal = {JOURNAL OF BUSINESS AND TECHNICAL COMMUNICATION},
Year = {2015},
Volume = {29},
Number = {2},
Pages = {184-220},
Month = {APR},
Abstract = {Many studies have examined organizations' use of specific types of
   online media, but few studies have examined how organizations generate
   dialogues and develop relationships by using multiple online
   communication platforms. This study takes an integrated approach by
   examining how top global organizations incorporate brand Web sites,
   Facebook, and Twitter to cultivate relationships with stakeholders. Its
   findings suggest that those particular online media are used similarly,
   that is, more for information dissemination than user engagement and
   more for one-way than two-way communication. The findings also suggest
   that the types of products promoted can affect the way that
   organizations use different online media to develop relationships.},
Type = {Article},
DOI = {10.1177/1050651914560569},
